// program for increasing numbers using recursion
class Increaserecursion{
    public static void printinc(int num){ // num=range of numbers
         if(num==1)//base case 
         {
         System.out.print(1);
         return;
         }
         printinc(num - 1); // first all recusrion calls occurs than values are printed
         System.out.print(" "+num+" ");

    }
    public static void main(String[] args) {
        
        int n = 10;
        printinc(n);
    }
} 
